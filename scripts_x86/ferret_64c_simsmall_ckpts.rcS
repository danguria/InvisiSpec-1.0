#!/bin/sh

# File to run the ferret benchmark

echo "Start ..."
cd /parsec/install/bin.ckpts
/sbin/m5 dumpstats
/sbin/m5 resetstats
./ferret /parsec/install/inputs/ferret/corels lsh /parsec/install/inputs/ferret/queriess 10 20 64 /parsec/install/inputs/ferret/output.txt
echo "Done :D"
/sbin/m5 exit
/sbin/m5 exit
